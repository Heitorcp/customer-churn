{
  "name": "Telco Customer Churn Prediction",
  "description": "A machine learning application for predicting customer churn using ML models with a Streamlit frontend",
  "keywords": [
    "machine-learning",
    "churn-prediction",
    "streamlit",
    "python",
    "data-science"
  ],
  "website": "https://github.com/Heitorcp/customer-churn",
  "repository": "https://github.com/Heitorcp/customer-churn",
  "logo": "https://streamlit.io/images/brand/streamlit-logo-secondary-colormark-darktext.png",
  "success_url": "/",
  "stack": "container",
  "formation": {
    "web": {
      "quantity": 1,
      "size": "basic"
    }
  },
  "env": {
    "PYTHONPATH": {
      "description": "Python path for the application",
      "value": "/app"
    },
    "STREAMLIT_SERVER_HEADLESS": {
      "description": "Run Streamlit in headless mode",
      "value": "true"
    },
    "STREAMLIT_BROWSER_GATHER_USAGE_STATS": {
      "description": "Disable Streamlit usage stats",
      "value": "false"
    }
  },
  "buildpacks": [],
  "addons": []
}